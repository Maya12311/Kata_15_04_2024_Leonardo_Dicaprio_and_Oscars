public class Kata
{
  public static String leo(final int oscar)
  {
    String phrase = "";
    
    if(oscar == 88){
      phrase= "Leo finally won the oscar! Leo is happy";
    }else if(oscar == 86){
      phrase= "Not even for Wolf of wallstreet?!";
    }
    
    else if(oscar < 88 && oscar != 88 && oscar != 86){
            phrase= "When will you give Leo an Oscar?";

    } else if (oscar > 88) {
      phrase= "Leo got one already!";
      }
    return phrase;
  }
}
